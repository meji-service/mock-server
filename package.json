{
  "name": "@enhances/mock-server",
  "version": "0.5.1",
  "description": "mock 服务器, 开箱即用，无需重启,自动代理远程服务、提供静态web服务入口、零接入成本",
  "type": "commonjs",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "dev": "nodemon src/packages/index.js",
    "build": "emp build",
    "publish:prod": "pnpm build && npm publish"
  },
  "bin": {
    "enhances-mock": "./bin/server.js"
  },
  "keywords": [
    "mock",
    "mock server",
    "proxy",
    "dev-server"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@efox/emp": "^2.7.1",
    "@enhances/mock-server": "workspace:^",
    "@mock-server/core": "workspace:^",
    "@mock-server/share": "workspace:^",
    "@mock-server/utils": "workspace:^",
    "nodemon": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "bin",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meji-service/mock-server.git"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1"
  }
}